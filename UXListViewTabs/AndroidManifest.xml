<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.uiexamples"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppBaseTheme" >
        <activity
            android:name="com.example.uiexamples.MainActivity"
            android:label="@string/app_name" >            
        </activity>
        <activity
            android:name="com.example.uiexamples.TabsActivity"
            android:label="@string/app_name" >
        </activity>
        <activity android:name="com.example.uiexamples.tabs.SongsActivity" />

        <!-- Videos Activity -->
        <activity android:name="com.example.uiexamples.tabs.VideosActivity" />

        <!-- Photos Activity -->
        <activity android:name="com.example.uiexamples.tabs.PhotosActivity" />
        <activity
            android:name="com.example.uiexamples.Main"
            android:label="@string/title_activity_main" >
            
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            
        </activity>
    </application>

</manifest>